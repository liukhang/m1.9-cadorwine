/* Header */

.page-header {
    margin: 0 auto;
    max-width: 100%;
    padding: 25px 0 0;
    display: block;
    width: 100%;
}

.header-language-background {
    padding: 0 20px;
	border-bottom: 1px solid #999;
}
.header-language-background, .header-language-background a {
    background: rgba(0, 0, 0, 0) none repeat scroll 0 center;
    color: #3b3937;
}
.header-language-background .form-language label {
    display: none;
}
.page-header-container .store-language-container select {
    border: 0 none;
    font-size: 14px;
    font-weight: 800;
    height: 30px;
    max-width: 100%;
    padding: 0 5px 0 5px; 
}
.header-language-background .header_spedizioni {
    
    float: right;
    line-height: 30px;
    /*margin-right: 45px;*/
    padding: 0 15px 0 0;
}
.page-header-container .store-language-container {
    max-width: 35%;
    padding: 0;
    position: absolute;
    right: 10px;
    top: -28px;
	display:none;
}
.header-language-background .header_spedizioni p {
    letter-spacing: 1.5px;
    text-transform: none;
    font-weight: 700;
    font-size: 13px;    
	line-height: 30px;
	/*border-right: 1px solid #999;*/
	/*padding-right:22px;*/
}
.header-language-background .header_spedizioni p span.color {
    color: #7bbe2d;
    text-transform: uppercase;
}
.header-language-background .header_spedizioni p .icon {
    font-size: 18px;
    line-height: 26px;
    padding: 0 6px 0 0;
}

.account-cart-wrapper .account-login {
    display: inline-block;
    vertical-align: middle;
}
.account-cart-wrapper .account-login a.skip-account, .account-cart-wrapper .skip-link.skip-cart {
    color: #3b3937;
    font-size: 21px;
    padding: 0 12px;
}
.account-cart-wrapper .header-minicart {
    display: inline-block;
    vertical-align: middle;
}
.header-minicart .mini-products-list .item-options{ overflow: hidden;}
.header-minicart .mini-products-list .item-options dt {
    float: left;
    clear: left;
    font-weight: 700;
    padding-right: 5px;
    font-style: normal;
    font-size: 11px;
}
.header-minicart .mini-products-list .item-options dd {
    float: left;
    padding-left: 10px;
    margin: 0 0 0px;
    font-size: 11px;
}

.header-minicart .mini-price-cart .price{   
    font-weight: 400;
    font-size: 14px;}
.header-minicart .info-wrapper th {
    text-transform: capitalize;
    padding-right: 14px;
    font-size: 11px;
    color: #3b3937;
    font-weight: 300;
}
.header-minicart .info-wrapper .qty-wrapper td {
    height: 25px;
    line-height: 25px;
}
.header-minicart .info-wrapper .qty {
    padding-left: 4px;
    padding-right: 4px;
    margin-right: 2px;
    width: 3.2em;
    text-align: center;
    height: 20px;
    background: none;
    border: none;font-size: 11px;
}
.header-minicart .info-wrapper .quantity-button {padding: 3px 10px;}
.header-minicart .block-content {
    padding: 10px 0;
    margin-top: 0;
    border-top: 1px solid #fff;
    display: none;
}
.header-minicart .subtotal .label {
    color: #3b3937;
    font-size: 17px;
    font-weight: 500;
    letter-spacing: 2px;
    margin-right: 10px;
    text-transform: uppercase;
}
.header-minicart .subtotal .price {
    color: #3b3937;
    font-weight: 800;
    font-size: 17px;
}
.header-minicart .minicart-actions {
    padding: 10px 0;
}
.header-minicart .minicart-actions .checkout-button {
    min-width: auto;
    width: 100%;
    font-size: 14px;
    font-weight: 400;
    padding: 10px 20px;
    margin-bottom: 15px;
}
.header-minicart .minicart-actions .checkout-button:hover {
    color: #1f1f1f;
	background: rgba(0, 0, 0, 0) none repeat scroll 0 center;
}
.header-minicart .minicart-actions .cart-link {
    color: #1f1f1f;
    font-size: 17px;
    font-weight: 500;
    height: 33px;
    letter-spacing: 1px;
    line-height: 39px;
    text-decoration: underline;
    text-transform: uppercase;
}
.header-minicart .minicart-actions .oppure {
    color: #1f1f1f;
    font-size: 17px;
    font-weight: 500;
    height: 33px;
    line-height: 39px;
    text-transform: uppercase;
}
.header-minicart .minicart-actions .minicart { margin: 0;}

.page-header .skip-link.skip-search {
    display: none;
}
.page-header #header-search .ico-search {
    background: rgba(0, 0, 0, 0) none repeat scroll 0 center;
    color: #3b3937;
    display: block;
    font-size: 21px;
    padding: 0 12px;
    width: auto;
    z-index: 999;
}
.page-header #header-search {
    height: auto;
    right: 15px;
    top: 95px;
    width: 50px;
}
#header-search .search-container {
    position: absolute;
    right: 0;
    top: -5px;
    transition: all 0.3s linear 0s;
    width: 0;
    z-index: 99;
}
#header-search .search-container.open {
   width: 280px;
}
#header-search .search-container #search_mini_form input {
    border: none !important;
    border-bottom: 1px solid #3b3a38 !important;
    border-radius: 0;
    display: none;
    padding: 5px 45px 5px 15px;
    font-size: 12px;
    position: absolute;
    top: 55px;
    background: #fff;
}
#header-search .search-container #search_mini_form input:focus{
    outline: none;
    border: none !important;
    border-bottom: 1px solid #3b3a38 !important;
}
#header-search .search-container #search_mini_form input:focus, input[type="search"]:focus{
    border: 1px solid #3b3a38;
    outline: none;
}
#header-search #search_mini_form .search-btn {
    background: rgba(0, 0, 0, 0) none repeat scroll 0 center;
    border: 0 none;
    display: none;
    font-size: 21px;
    position: absolute;
    right: 18px;
    top: 5px;
}
#header-search .open #search_mini_form .search-btn {
    display: block;
}
.account-cart-wrapper {
    position: absolute;
    right: 75px;
    top: 90px;
}
.page-header #header-search.skip-content.skip-active {
    background: rgba(0, 0, 0, 0) none repeat scroll 0 center;
    display: block;
}
#header-nav .nav-primary .level1.view-all {
    display: none;
}
.nav-primary .menu-active > ul.level0 {
    background: #f2c02f none repeat scroll 0 0;
    border: 0 none;
}
.nav-primary .menu-active > ul.level0 li.level1 a {
    color: #fff;
    font-size: 14px;
    padding: 5px 25px;
    text-transform: capitalize;
    font-weight: 300;
}
.nav-primary .menu-active > ul.level0 li.level1:hover{
    background: #3b3937;
}
.customer-account-create .header-account-create .fieldset {
    margin: 0;
}
