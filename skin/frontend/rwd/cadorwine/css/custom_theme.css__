/*Cookie*/
	.cookie-box{
		display: none;
		position: fixed;
		z-index: 1000000000000000;
		background-color: rgba(0,0,0,.90);
		width:100%;
		bottom: 0;
		opacity: 0;
		padding: 5px 0 10px;
	}
	.cookie-container{
		width: 100%;
	    max-width: 1400px;
    	margin: 5px auto;
	}
	.container-text-cookie{
		margin-bottom: 4px;
	}
	.container-text-cookie p.cookie-text{
		color: #fff;
		font-size: 12px;
		margin: 0;
		text-align: center;
		font-weight: normal;
		line-height: 130%;
	}
	#containerPrivacy{
		float: left;
		text-align: right;
		width:49%;
		margin-right: 1%;
	}
	#containerPrivacy a#linkPrivacy{
		color: #eee;
		font-size:12px;
		line-height:130%;
		text-decoration:none;
		cursor:pointer;
		padding-top:4px;
		display:inline-block;
	}
	#containerPrivacy a#linkPrivacy:hover{
		color: #fff;
		border-bottom: 1px dotted #fff; 
	}
	#containerAccept{
		float: left;
		text-align: left;
		width: 49%;
		margin-left: 1%;
	}
	#containerAccept a#acceptCookie{
		background-color: #ebebeb;
		color: #222222;
		padding:3px 16px;
		display: inline-block;
		font-size: 12px;
		line-height: 130%;
		text-decoration: none;
		-webkit-border-radius: 2px;
		-moz-border-radius: 2px;
		-o-border-radius: 2px;
		-ms-border-radius: 2px;
		border-radius: 2px;
		cursor: pointer;
	}
	#containerAccept a#acceptCookie:hover{
		background-color: #ffffff;
		color: #222222;
	}
@media only screen and (max-width: 520px){
	.container-text-cookie{
		float: left;
		width: 59%;
		margin-left: 1%
	}
	.container-text-cookie p.cookie-text{
		text-align:left;
	}
	#containerPrivacy {
    	float: right;
    	text-align: center;
    	width: 39%;
    	margin-right: 1%;
    	margin-bottom:7px;
	}
	#containerAccept {
    	float: left;
    	text-align: center;
    	width: 39%;
    	margin-left: 1%;
   	}
	#containerAccept a#acceptCookie{
		width: 70px;
		padding: 8px 16px;
	}
}
@media only screen and (max-width: 520px){
	.container-text-cookie {
		width: 50%;
		margin-bottom: 0;
		margin-top: 6px;
	}
	#containerPrivacy {
    	width: 48%;
	}
	#containerAccept {
    	width: 48%;
	}
}
